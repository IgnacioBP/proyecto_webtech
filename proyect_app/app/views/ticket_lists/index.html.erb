<p style="color: green"><%= notice %></p>
<div class="text-center">
<h1>Ticket lists</h1>
</div>

<div id="ticket_lists">
  <% @ticket_lists.each do |ticket_list| %>
  
  <% if ticket_list.user.mail == params[:user_mail]%>
  <div class="card mx-auto w-50 p-3 bg-white text-black text-center">
    <%= render ticket_list %>
    <p>
      <%= link_to "Show this ticket list", user_ticket_list_path(ticket_list.user_mail,ticket_list) %>
    </p>
    </div>
  <br>
  <% end %>
  
  <%end%>
 
</div>
 <div class="card mx-auto w-50 p-3 bg-white text-black text-center">
  <%= link_to "Back to user", user_path(params[:user_mail]) %>
  <%= link_to "New ticket list", new_user_ticket_list_path %>
</div>
